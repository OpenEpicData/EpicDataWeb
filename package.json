{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "ui": "umi dev UMI_UI=1",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write .",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
