    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/colors": "^4.0.5",
    "@ant-design/pro-layout": "^5.0.12",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@mui-treasury/components": "^1.7.0",
    "@mui-treasury/layout": "^4.5.0",
    "@mui-treasury/mockup": "^1.2.6",
    "@mui-treasury/styles": "^1.10.0",
    "@mui-treasury/styling": "^0.2.8",
    "@mui-treasury/utils": "^0.4.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.24",
    "lint-staged": "^10.0.7",
    "prettier": "2.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-google-font-loader": "^1.1.0",
    "umi": "^3.2.24",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {}
